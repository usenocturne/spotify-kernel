	 *	};
	 *};
	 */
	/* Spotify edit */
	spotify_buttons: spotify_buttons {
		mux { /* GPIOA_5 GPIOA_6 */
			pins = "GPIOA_0",
				"GPIOA_1",
				"GPIOA_2",
				"GPIOA_3",
				"GPIOAO_3",
				"GPIOA_5";
			bias-disable;
			input-enable;
		};
	};
	/* Spotify edit */
	spotify_rotary: spotify_rotary {
		mux {
			pins = "GPIOZ_7",
				"GPIOZ_8",
				"GPIOZ_9";
			bias-disable;
			input-enable;
		};
	};

	/* Spotify edit */
	spotify_touch_i2c: spotify_touch_i2c {
		mux { /* GPIOZ_0 GPIOZ_1 */
			groups = "i2c0_sda_z0", "i2c0_sck_z1";
			function = "i2c0";
                        drive-strength = "1";
		};
	};

	spotify_touch_gpio: spotify_touch_gpio {
		mux {
			groups = "GPIOZ_3", "GPIOZ_4";
			function = "gpio_periphs";
                        drive-strength = "1";
		};
	};

	spotify_prox_gpio: spotify_prox_gpio {
		mux {
			pins = "GPIOZ_10";
			bias-pull-down;
		};
		/*
		mux1 {
			pins = "GPIOZ_11";
			bias-pull-up;
		};
		*/
	};

	spotify_imu_gpio: spotify_imu_gpio {
		mux {
			pins = "GPIOZ_12", "GPIOZ_13";
			bias-pull-down;
		};
	};

	spotify_lcd_reset_pin: spotify_lcd_reset_pin {
		mux {
			pins = "GPIOZ_5";
			function = "gpio_periphs";
			bias-disable;
                        drive-strength = "1";
		};
	};

	spotify_mfi_i2c_pins: spotify_mfi_i2c_pins {
		mux {
			groups = "i2c3_sda_a",
			"i2c3_sck_a";
			function = "i2c3";
			bias-disable;
			drive-strength = <2>;
		};
	};

	spotify_bluetooth_bt_wake_host: spotify_bluetooth_bt_wake_host {
		pins = "GPIOX_6";
		bias-disable;
		input-enable;
	};

	spotify_bluetooth_host_wake_bt: spotify_bluetooth_host_wake_bt {
		pins = "GPIOX_7";
		bias-disable;
		output-enable;
                drive-strength = "1";
	};

	spotify_disabled_pins_c: spotify_disabled_pins_c {
		mux {
			pins = "GPIOC_0",
				"GPIOC_1",
				"GPIOC_2",
				"GPIOC_3",
				"GPIOC_5",
				"GPIOC_6",
				"GPIOC_7";
			bias-pull-down;
			output-low;
		};
	};

	spotify_disabled_pins_a: spotify_disabled_pins_a {
		mux {
			pins = "GPIOA_10",
				"GPIOA_11",
				"GPIOA_12",
				"GPIOA_13";
			bias-pull-down;
			output-low;
		};
	};

	spotify_disabled_pins_h: spotify_disabled_pins_h {
		mux {
			pins = "GPIOH_0",
				"GPIOH_1",
				"GPIOH_2",
				"GPIOH_3",
				"GPIOH_6",
				"GPIOH_7",
				"GPIOH_8";
			bias-pull-down;
			output-low;
		};
	};

	tp_int_input: eint@0 {
		mux {
			pins = "GPIOZ_4";
			bias-pull-up;
		};
	};

	state_rst_output0: rstoutput0 {
		mux {
			pins = "GPIOZ_3";
			output-low;
			bias-pull-down;
                        drive-strength = "1";
		};
	};

	state_rst_output1: rstoutput1 {
		mux {
			pins = "GPIOZ_3";
			output-high;
			bias-pull-up;
                        drive-strength = "1";
		};
	};

	/* Spotify edit */
	pdmin: pdmin {
		mux {
			groups = "pdm_din0_a",
