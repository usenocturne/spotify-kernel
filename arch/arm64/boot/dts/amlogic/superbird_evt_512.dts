	     compatible = "generic-adc-thermal";
	     #thermal-sensor-cells = <0>;
	     io-channels = <&saradc SARADC_CH0>;
	     io-channel-names = "sensor-channel";
	     temperature-lookup-table = < 85000  894
                                          80000    878
                                          75000    860
                                          70000    839
                                          65000    815
                                          60000    789
                                          55000    759
